<launch>

  
  <arg name="home_dir" default="/home/appinho"/>

  <arg name="scenario" default="0059"/>
  
  <node pkg="stereo_image" type="stereo_image_node.py" name="stereo_image_node" output="screen">
  </node>

  
  <node pkg="rosbag" type="play" name="player" args="-r 0.51 -d 2 $(arg home_dir)/kitti_data/$(arg scenario)/synchronized_data.bag"/>

  
  <include file="$(find stereo_image)/launch/tools.launch"/>
  <!--
  <node pkg="rviz" type="rviz" name="my_rviz"
	args="-d $(find stereo_image)cfg/stereo.rviz"/>  
  -->
</launch>
